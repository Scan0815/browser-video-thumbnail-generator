{
  "name": "browser-video-thumbnail-generator",
  "version": "1.2.2",
  "description": "This is a library for generating thumbnails from video files. It provides a simple API for extracting a single frame or multiple frames from a video and returning them as images.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/browser-video-thumbnail-generator/browser-video-thumbnail-generator.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Scan0815/browser-video-thumbnail-generator.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "prepare": "npm run build --prod",
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "^20.7.3",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "license": "MIT"
}
